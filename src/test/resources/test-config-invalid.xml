<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="test-config.xsd">
	<!-- smtp configuration -->
	<!-- securitytype supported: NONE, SSL_TLS and STARTTLS -->
	<!-- authtype supported: NONE and PASSWORD -->
	<mail>
		<enabled>false</enabled>
		<server>smtp.test.tld</server>
		<port>587</port>
		<username>username</username>
		<password>password</password>
		<authtype>PASSWORD</authtype>
		<securitytype>STARTTLS</securitytype>
		<from>test@test.tld</from>
	</mail>
	
	<!-- device configurations (max. 20 allowed) -->
	<!-- timeout property is optional --> 
	<!-- every configuration must be unique -->
	<device>
		<ip>invalid</ip>
		<name>Test machine</name>
		<interval>355</interval>
		<limit>37</limit>
		<maxgraph>500</maxgraph>
		<email>test@test.com</email>
	</device>
	<device>
		<ip>127.0.0.1</ip>
		<name>Test machine 2</name>
		<interval>355</interval>
		<limit>37</limit>
		<maxgraph>500</maxgraph>
		<email>invalid.com</email>
	</device>
	<device>
		<ip>127.0.0.1</ip>
		<name>Test machine 3</name>
		<interval>355</interval>
		<limit>37</limit>
		<maxgraph>500</maxgraph>
		<email>test@test.com</email>
	</device>
</config>
